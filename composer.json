{
    "name": "sudhanshu-mittal/telr",
    "description": "Pay online through telr payment gateway",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "laravel",
        "payment",
        "telr payment",
        "telr"
    ],
    "authors": [{
        "name": "Sudhanshu Mittal",
        "email": "mittalsudhanshu29@gmail.com"
    }],
    "autoload": {
        "psr-4": {
            "TelrGateway\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TelrGateway\\TelrServiceProvider"
            ]
        }
    },
    "scripts": {
